{
  "author": "Ryan Garant",
  "license": "MIT",
  "name": "floorplan",
  "repository": {
    "type": "git",
    "url": "https://github.com/protoEvangelion/interactiveTradeFloor.git"
  },
  "version": "2.0.0",
  "private": true,
  "main": "src",
  "scripts": {
    "env": "cross-env NODE_ENV=production MONGODB_URI=mongodb://localhost/floorplan PORT=3000",
    "start": "npm run env node .",
    "build:clean": "rimraf \"dist/!(.git*|Procfile)**\"",
    "build:copy": "copyfiles -u 1 public/* public/**/* dist",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config",
    "coverage:lean": "npm run test:lean -- --coverage && npm run postcoverage",
    "coverage": "npm test -- --coverage",
    "dev": "cross-env NODE_ENV=development concurrently --kill-others \"nodemon --watch src\" \"node webpack/webpack-dev-server\"",
    "heroku-postbuild": "npm run build",
    "lint": "eslint src src-clean .storybook",
    "mongo:log": "cat /var/log/mongodb/mongod.log",
    "mongo:restart": "sudo service mongod restart",
    "mongo:restore": "mongorestore -h ds249575.mlab.com:49575 -d heroku_n9739n51 -u heroku_n9739n51 -p <password> lbBooths.bson",
    "mongo:showla": "bsondump --pretty laBooths.bson",
    "mongo:showlb": "bsondump --pretty lbBooths.bson",
    "mongo:start": "sudo service mongod start",
    "mongo:stop": "sudo service mongod stop",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "prebuild": "npm run build:clean && npm run build:copy",
    "prepublish": "npm run snyk-protect",
    "snyk-protect": "snyk protect",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test:lean": "cross-env NODE_ENV=test jest -o --runInBand --forceExit --watch",
    "test": "cross-env NODE_ENV=test jest --runInBand --forceExit"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/src-example"
    ],
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/fileMock.js",
      "^(components|containers)$": "<rootDir>/test/componentsMock.js",
      "^(store|\\.\\.)\\/selectors$": "<rootDir>/test/selectorsMock.js",
      "^(store|\\.\\.)\\/actions$": "<rootDir>/test/actionsMock.js"
    }
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.2",
    "babel-eslint": "^7.1.1",
    "concurrently": "^3.1.0",
    "enzyme": "^2.7.0",
    "eslint": "^3.14.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-no-unused-vars-rest": "^1.0.4",
    "eslint-plugin-react": "^6.9.0",
    "jest-cli": "^19.0.0",
    "mockgoose": "^7.0.6",
    "nock": "^9.0.2",
    "nodemon": "^1.11.0",
    "opn-cli": "^3.1.0",
    "prop-types": "^15.6.0",
    "react-addons-test-utils": "^15.4.1",
    "react-test-renderer": "^15.5.4",
    "redux-mock-store": "^1.2.1",
    "sinon": "^2.1.0",
    "supertest": "^2.0.1",
    "webpack-dev-server": "^2.9.4"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.2.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "body-parser": "^1.15.2",
    "bodymen": "^1.0.3",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "copyfiles": "^1.0.0",
    "cors": "^2.8.1",
    "cross-env": "^3.1.4",
    "csurf": "^1.9.0",
    "dotenv": "^4.0.0",
    "dotenv-parse-variables": "^0.1.0",
    "express": "^4.14.0",
    "express-force-ssl": "^0.3.2",
    "file-loader": "^0.10.0",
    "history": "3.0.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.4",
    "material-ui": "^0.17.1",
    "mongoose": "^4.7.6",
    "mongoose-keywords": "^0.3.2",
    "morgan": "^1.7.0",
    "nodemailer": "^3.1.8",
    "normalizr": "^3.0.1",
    "progress-bar-webpack-plugin": "^1.9.3",
    "qs": "^6.4.0",
    "querymen": "^2.1.1",
    "raw-loader": "^0.5.1",
    "react": "^15.4.1",
    "react-cookie": "^1.0.4",
    "react-dom": "^15.4.1",
    "react-helmet": "^4.0.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-modal": "^1.6.5",
    "react-onclickoutside": "^5.10.0",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-tooltip": "^3.2.9",
    "redux": "^3.6.0",
    "redux-form": "^6.4.3",
    "redux-form-material-ui": "^4.1.2",
    "redux-form-submit": "^0.1.3",
    "redux-saga": "^0.14.1",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.6.1",
    "serialize-javascript": "^1.3.0",
    "snyk": "^1.42.3",
    "socket.io": "^2.0.2",
    "socket.io-client": "^2.0.2",
    "styled-components": "^1.4.4",
    "styled-theme": "^0.3.0",
    "styled-tools": "^0.1.2",
    "url-loader": "^0.6.0",
    "url-parse": "^1.1.8",
    "validator": "^9.4.1",
    "webpack": "^2.2.1",
    "webpack-isomorphic-tools": "^3.0.0",
    "webpack-md5-hash": "^0.0.5"
  },
  "snyk": true
}
